{
    "name": "hash-technologies/thaana-textarea",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "HashTechnologies\\ThaanaTextarea\\": "src/"
        }
    },
    "extra": {
